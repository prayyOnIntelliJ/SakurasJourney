void fragment() {
// Vector2Parameter:22
	vec2 n_out22p0 = Scale;


// UVFunc:21
	vec2 n_in21p2 = vec2(0.00000, 0.00000);
	vec2 n_out21p0 = (UV - n_in21p2) * n_out22p0 + n_in21p2;


// Input:18
