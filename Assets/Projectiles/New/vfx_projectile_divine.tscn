	vec2 n_out17p0 = n_out20p0 * n_in17p1 + n_out21p0;


	vec4 n_out3p0;
// Texture2D:3
	n_out3p0 = texture(Texture_1, n_out17p0);
	float n_out3p1 = n_out3p0.r;


	vec4 n_out7p0;
// Texture2D:7
	n_out7p0 = texture(Texture_2, UV);
	float n_out7p1 = n_out7p0.r;


// FloatOp:23
	float n_out23p0 = n_out3p1 + n_out7p1;


	vec4 n_out10p0;
// Texture2D:10
	n_out10p0 = texture(Texture_3, UV);
	float n_out10p1 = n_out10p0.r;


// FloatOp:27
	float n_in27p0 = 1.00000;
	float n_out27p0 = n_in27p0 * n_out10p1;


// FloatOp:24
	float n_out24p0 = n_out23p0 * n_out27p0;


	vec4 n_out14p0;
// Texture2D:14
	n_out14p0 = texture(Texture_5, UV);
	float n_out14p1 = n_out14p0.r;


// FloatOp:25
	float n_out25p0 = n_out24p0 - n_out14p1;


// Clamp:26
	float n_in26p1 = 0.00000;
	float n_in26p2 = 1.00000;
	float n_out26p0 = clamp(n_out25p0, n_in26p1, n_in26p2);


	vec4 n_out12p0;
// Texture2D:12
	n_out12p0 = texture(Texture_4, vec2(n_out26p0));
	float n_out12p4 = n_out12p0.a;


// Output:0
	COLOR.rgb = vec3(n_out12p0.xyz);
	COLOR.a = n_out12p4;


}
